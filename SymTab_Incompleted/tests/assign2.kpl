PROGRAM ASSIGN2;
VAR A : INTEGER;
    B : INTEGER;
    SUM : INTEGER;

PROCEDURE INPUTANDSUM;
BEGIN
  CALL WRITEC('A');
  CALL WRITEC('=');
  A := READI;
  
  CALL WRITEC('B');
  CALL WRITEC('=');
  B := READI;
  
  SUM := A + B;
  
  CALL WRITEI(SUM);
  CALL WRITELN
END;

BEGIN
  CALL INPUTANDSUM
END.