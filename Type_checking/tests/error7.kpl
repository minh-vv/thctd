PROGRAM ERROR7;  (* Example 6 - Modified *)
   CONST C1 = 10;
  	 C2 = 'a';
   TYPE T1 = ARRAY(. 10 .) OF INTEGER;
   VAR V1 : INTEGER;
       V2 : ARRAY(. 10 .) OF T1;

   (* Tham số P2 là tham biến VAR P2 : INTEGER *)
   FUNCTION F(P1 : INTEGER; VAR P2 : INTEGER) : INTEGER;
     BEGIN
       F := C1;
     END;

   PROCEDURE P(V1 : INTEGER);
     CONST C1 = 'a';
           C3 = 10;
     TYPE T1 = INTEGER;
          T2 = ARRAY(. 10 .) OF T1;
     VAR V2 : T2;
         V3 : CHAR;
         X : INTEGER;
         Y : INTEGER;
     BEGIN
	V3 := 'a';
        X := 5;
        Y := 10;
        V1 := F(C3, (X + Y) * 2);  (* LỖI: Tham số thứ 2 là tham biến nhưng truyền biểu thức X + Y *)
     END;

BEGIN
     CALL P(C1);
END.  (* Error 7 *)
